{
  "buildCommand": "echo 'Building...'",
  "redirects": [
    {
      "source": "/character-creation.html",
      "destination": "/character-sheet.html",
      "permanent": true
    },
    {
      "source": "/character-creation",
      "destination": "/character-sheet.html",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/onboarding",
      "destination": "/onboarding.html"
    },
    {
      "source": "/attribute-method",
      "destination": "/attribute-method.html"
    },
    {
      "source": "/roll-4d6",
      "destination": "/roll-4d6.html"
    },
    {
      "source": "/distribute-attributes",
      "destination": "/distribute-attributes.html"
    },
    {
      "source": "/character-sheet",
      "destination": "/character-sheet.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
